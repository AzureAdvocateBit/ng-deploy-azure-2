{
  "name": "ng-deploy-az",
  "version": "0.0.0",
  "description": "A blank schematics",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "git checkout HEAD -- sandbox && git clean -f -d sandbox",
    "link:schematic": "npm link && cd sandbox && npm link \"ng-deploy-az\"",
    "sandbox:ng-add": "cd sandbox && ng g ng-deploy-az:ng-add",
    "test": "npm run clean && npm run sandbox:ng-add && npm run test:sandbox",
    "test:unit": "npm run build && jasmine src/**/*_spec.js",
    "test:sandbox": "cd sandbox && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./src/collection.json",
